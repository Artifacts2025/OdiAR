# OdiAR Environment Variables
# Copy this file to .env and update with your actual values

# Database Configuration
DB_HOST=localhost
DB_USER=root
DB_PASSWORD=your_mysql_password
DB_NAME=odiar_db
DB_PORT=3306

# API Configuration
API_PORT=3001
NODE_ENV=development

# CDN/Storage URLs (for production)
CDN_BASE_URL=https://cdn.odiar.app
MEDIA_BASE_URL=https://media.odiar.app

# Google Maps API (for maps feature)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Analytics (optional)
ANALYTICS_ID=your_analytics_id
